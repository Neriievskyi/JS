1. 'number' + 3 + 3 = number33
//конкатенація: 'number' - строка, число 3 приведене до строки 3

2. null + 3 = 3
//null приведе до числа 0, - далі математичне додавання

3. 5 && "qwerty" = qwerty
//логічне && перевіряє операнди зліва направо, якщо лівий true - повертає правий, якій теж true

4. +'40' + +'2' + "hillel" = 42hillel
//+ перед строкой ’40' приводить її до числа, + перед строкой '2' приводить її до числа, далі вони сумуються, потім число приводться до строки, - потім конкатенація

5. '10' - 5 === 6 = false
//строка ’10' приводиться до числа, далі віднімання 10 - 5 = 5; 5 !== 6. Ітог: false

6. true + false = 1
//true приводиться до числа 1, а false до 0, далі додавання 1 + 0 = 1

7. '4px' - 3 = NaN
//js намагається перетворити строку '4px' на число, але т.я. в строкі є ‘px' не може це зробити і повертає not-a-number

8. '4' - 3 = 1
//'4' строка перетворюється на число 4, потім віднімання 4 - 3 = 1

9. '6' + 3 ** 0 = 61
//3 ** 0 означає 3 підняти до нульового ступеня - 1, потім число 1 приводиться до строки, далі конкатенація

10. 12 / '6' = 2
//оператор / приводить строку до числа 6, далі 12 ділить на 6 = 2

11. '10' + (5 === 6) = 10false
//(5 === 6) буде false, далі конкатенація строки 10 і строки false

12. null == '' = false
//null не приводиться до порожньої строки за допомогою оператора ==
//null не дорівнює порожній строкі, тому false

13. 3 ** (9 / 3) = 27
//результат (9 / 3) буде 3, 3 в ступені 3 = 27

14. !!'false' == !!’true' = true
//!! перетворює значення в boolean еквівалент, !!’false' стає true, т.я. це не порожній рядок
//!!’true’ це true, результат теж true

15. 0 || '0' && 1 = 1
//&& має вищий пріоритет ніж ||, тому спочатку виконується вираз '0' && 1 буде 1, далі 0 || 1 буде 1, т.я. || повертає перше true значення, це 1

16. (+null == false) < 1 = false
//унарний плюс перетворює значення на число, null стає 0
//0 == false повертає true
//в true < 1 true перетворюється на число 1
//1 < 1 повертає false



17. false && true || true = true
//false && true повертає false
//false || true повертає true


18. false && (false || true) = false
//результат (false || true) буде true
//далі результат false && true буде true

19. (+null == false) < 1 ** 5 = false
//+null пертворюється на число 0
//далі 0 == false повертає true, т.я. false приводиться до числа 0
//далі 1 ** 5 це 1 в 5му ступепні = 1
//true рпиводиться до числа 1 і 1 < 1 поверне false

